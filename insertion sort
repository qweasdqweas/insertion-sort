package project;

import java.util.Arrays;

public class project {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i, j, temp;
		
		int arr [] = {3,1,4,2,5} ;
		
		for(i=0; i<arr.length; i++) {
		
			temp = arr[i];
			
			for(j=i-1; j>0 && arr[j] > temp; j--)
				arr[j+1] = arr[j];
					arr[j+1] = temp;
						
		}
		
		System.out.println(Arrays.toString(arr));
		
	}

}
